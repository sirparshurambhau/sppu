import tensorflow as tf
import numpy as np

# Define input data
input_data = np.array([[0, 0], [0, 1], [1, 0], [1, 1]], dtype=np.float32)

# Define target data
and_labels = np.array([[0], [0], [0], [1]], dtype=np.float32)

# Define the model
model_and = tf.keras.Sequential([
    tf.keras.layers.Dense(units=1, input_shape=(2,), activation='sigmoid')
])

# Compile the model
model_and.compile(optimizer='adam', loss='binary_crossentropy', metrics=['accuracy'])

# Train the model
model_and.fit(input_data, and_labels, epochs=1000, verbose=0)

# Test the model
and_predictions = model_and.predict(input_data)
print("AND Gate Predictions:")
print(and_predictions)
